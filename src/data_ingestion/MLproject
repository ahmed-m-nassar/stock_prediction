name: data_ingestion

entry_points:
  main:
    parameters:
      stock_name:
        description: stock name to download its data
        type: string

      output_artifact:
        description: output data name in wandb
        type: string

      output_type:
        description: type of output data
        type: string

      output_description:
        description: description of the output data
        type: string


    command: >-
        python data_ingestion.py  --stock_name {stock_name}  --output_artifact {output_artifact}  --output_type {output_type}  --output_description {output_description} 
